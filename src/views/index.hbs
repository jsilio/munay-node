<section id="blog" class="py-4 bg-light">
  <div class="container">

    {{!--  Título y Descripción --}}
    <div class="row justify-content-center">
      <div class="col-12 col-md-10 col-lg-8 text-center">

        <h2 class="font-weight-bold">Últimas entradas en nuestro blog</h2>

        <p class="font-size-lg text-gray-700 mb-7 mb-md-9">
          Explicamos de forma clara y sencilla conceptos de psicología útiles para tu día a día.
          Recogemos los temas que más dudas generan.
          Exponemos algunas publicaciones recientes en revistas científicas.
        </p>

      </div>
    </div> <!-- / .row -->

    {{!-- Entradas del Blog. Pendiente: hacer un forEach de los BlogPost.js en la db --}}
    <div class="row">

      {{#each blogPost}}
      <!-- /// Entrada 1 /// -->
      <div class="col-12 col-lg-4 d-flex my-5">

        <!-- Card -->
        <div class="card mb-6 mb-lg-0 shadow-light-lg lift lift-lg">

          <!-- Imagen -->
          <a class="card-zoom" href="/blog/{{slug}}">
            <img src="{{coverURL}}" alt="..." class="img-fluid card-img-top">
          </a>

          <!-- Shape -->
          <div class="position-relative">
            <div class="shape shape-fluid-x shape-bottom svg-shim text-white">
              <svg viewBox="0 0 2880 48" fill="none">
                <path d="M0 48H1437.5H2880V0H2160C1442.5 52 720 0 720 0H0V48Z" fill="currentColor" />
              </svg>
            </div>
          </div>

          <!-- Body -->
          <a class="card-body" href="/blog/{{slug}}">

            {{!-- <!-- Categoría de la entrada -->
                        <div class="position-relative text-right mt-n8 mr-n4 mb-3">
                            <span class="badge badge-pill badge-primary">
                                <span class="h6 text-uppercase">Neuropsicología</span>
                            </span>
                        </div> --}}

            <!-- Heading -->
            <h3> {{title}} </h3>

            <!-- Text -->
            <p class="mb-0 text-muted">
              {{description}}
            </p>
          </a>

          <!-- Meta -->
          <a class="card-meta mt-auto" href="#!">

            <!-- Divisor -->
            <hr class="card-meta-divider">

            <!-- Avatar -->
            <div class="avatar avatar-sm mr-2">
              <img src="/img/photos/laurita.jpg" alt="Laura Marín Psicóloga Clínica Munay"
                class="avatar-img rounded-circle">
            </div>

            <!-- Autor -->
            <h6 class="text-uppercase text-muted mr-2 mb-0"> Laura Marín </h6>

            <!-- Fecha -->
            <p class="h6 text-uppercase text-muted mb-0 ml-auto">
              {{date createdAt "L"}}
            </p>

            <i class="fe fe-calendar text-muted ml-2 mb-1"></i>
          </a>

        </div>

      </div>
      {{/each}}


    </div> <!-- / .row -->
  </div> <!-- / .container -->
</section>


